# Test Environment Variables Example
# Copy this file to .env.test and update values for your test environment

# Database - Use a separate test database
DATABASE_URL=postgresql://maestro:maestro123@localhost:5432/maestro_test?schema=public

# Redis
REDIS_URL=redis://localhost:6379

# Authentication - Use different secrets for testing
JWT_SECRET=test-jwt-secret-change-this-for-your-tests

# OAuth Placeholders (not required for most tests)
GOOGLE_CLIENT_ID=test-google-client-id
GOOGLE_CLIENT_SECRET=test-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3001/api/auth/google/callback
GITHUB_CLIENT_ID=test-github-client-id
GITHUB_CLIENT_SECRET=test-github-client-secret
GITHUB_CALLBACK_URL=http://localhost:3001/api/auth/github/callback

# URLs
FRONTEND_URL=http://localhost:3000
API_URL=http://localhost:3001
CORS_ORIGINS=http://localhost:3000

# Environment
NODE_ENV=test

# File Upload
MAX_FILE_SIZE=5242880

# Email Placeholders (not required for most tests)
SMTP_HOST=smtp.test.com
SMTP_PORT=587
SMTP_USER=test@test.com
SMTP_PASS=test-password
SMTP_FROM=noreply@test.com
